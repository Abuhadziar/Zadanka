#include <vector>
#include <bits/stdc++.h>
int josephusSurvivor(int n, int k) {
  std::vector<int> tab;
  for (int i=1;i<=n;i++){
    tab.push_back(i);
  }
  while (tab.size()>=k){
    for (size_t i=0;i<tab.size();i++){
      if (i%k==0){
        tab.erase(tab.begin()+i);
      }
    }
  }

  
  
  return ;
}
